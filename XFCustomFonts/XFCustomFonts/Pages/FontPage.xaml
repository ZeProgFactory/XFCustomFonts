<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="XFCustomFonts.Pages.FontPage">
   <ContentPage.Content>
      <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
         <StackLayout Orientation="Horizontal" >
            <Label FontSize="32" Text="IconFont" Margin="10" VerticalTextAlignment="Center" WidthRequest="250" HorizontalTextAlignment="End" />
            <Label x:Name="labelIF" FontSize="64" Text="Welcome to Xamarin.Forms!" />
         </StackLayout>
         <StackLayout Orientation="Horizontal" >
            <Label FontSize="32" Text="MediaPlayerFont" Margin="10" VerticalTextAlignment="Center" WidthRequest="250" HorizontalTextAlignment="End"/>
            <Label x:Name="labelMPF" FontSize="64" Text="Welcome to Xamarin.Forms!" />
         </StackLayout>
      </StackLayout>
   </ContentPage.Content>
</ContentPage>